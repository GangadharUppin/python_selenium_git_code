pipeline {
    agent { label 'ubuntu' } // matches your EC2 agent label

    stages {
        stage('Checkout') {
            steps {
                echo 'checkout job'
                sh 'pwd && ls -l'
                git(
                    url: 'https://github.com/GangadharUppin/python_selenium_git_code.git',
                    branch: 'master',
//                     credentialsId: 'your-credential-id' // if private repo
                )
            }
        }
    }
}
