pipeline {
    agent {
        label 'ubuntu' // This should match the label of your Ubuntu EC2 agent
    }

    stages {
        stage('Clone Repository') {
            echo 'now we are cloning repo'
            steps {
                sh 'git clone https://github.com/GangadharUppin/python_selenium_git_code.git'
            }
        }
    }
}
