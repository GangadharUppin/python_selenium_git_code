pipeline {
    agent any

    environment {
        REPO_URL = 'https://github.com/GangadharUppin/python_selenium_git_code.git'
    }

    stages {
        stage('Clone Repository') {
            steps {
                git url: "${REPO_URL}"
            }
        }

        stage('Print Message') {
            steps {
                echo "Repository cloned successfully!"
            }
        }
    }
}
